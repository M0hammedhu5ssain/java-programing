class Main {
    public static void main(String[] args) {
    int arr[]={2,3,-2,4};

    int max=0;
    for(int i=0;i<arr.length;i++){
            int product=1;
        for(int j=i;j<arr.length;j++){
        product*=arr[j];
            max=Math.max(product,max);
        }
        
    }
    System.out.print(max);
    }
}
