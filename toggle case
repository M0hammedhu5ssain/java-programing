class Main {
    public static void main(String[] args) {
        String s = "Anna";
        String toggled = "";

        for (int i = 0; i < s.length(); i++) {
            char c = s.charAt(i);
            if (Character.isUpperCase(c)) {
                toggled += Character.toLowerCase(c);
            } else {
                toggled += Character.toUpperCase(c);
            }
        }

        System.out.println("Toggled case: " + toggled);
    }
}
