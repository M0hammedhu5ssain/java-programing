delete the vowels from the string

class Main {
    public static void main(String[] args) {
        String s = "Hello Java";
       // int count = 0;
        String sw="";

        for (int i = 0; i < s.length(); i++) {
            char c = Character.toLowerCase(s.charAt(i));
            if (c != 'a' && c != 'e' && c != 'i' && c != 'o' && c != 'u') {
                sw+=c;
            }
        }

        System.out.println("remove the vowels: " + sw);
    }
}
