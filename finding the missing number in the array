import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<arr.length;i++){
            arr[i]=sc.nextInt();
            
        }
        int a=arr.length;
    int sum=0;
    int naturalnum=n*(n+1)/2;
    for(int i=0;i<a;i++){
        sum+=arr[i];
        
    }
    System.out.println(naturalnum-sum);
    }
}
