import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int arr[]=new int[a];
        for(int i=0;i<arr.length;i++){
            arr[i]=sc.nextInt();
            
        }
        int min=Integer.MAX_VALUE;
        for(int i=0;i<arr.length;i++){
            if(min>=arr[i]){
                min=arr[i];
            }
        }
        System.out.println(min);
    }
}
