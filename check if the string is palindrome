
class Main {
    public static void main(String[] args) {
        String s="anna";
        String reverse="";
        for(int i=s.length()-1;i>=0;i--){
           char c= s.charAt(i);
           reverse+=c;
            
        }
        if(s.equals(reverse)){
            System.out.print("this is palindriome");
        }
        else{
            System.out.print("no");
        }
                    
        
       
    }
}
